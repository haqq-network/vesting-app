{
  "name": "@haqq/vesting-app",
  "version": "0.1.0",
  "homepage": "https://vesting.haqq.network",
  "repository": {
    "type": "git",
    "url": "https://github.com/haqq-network/vesting-app.git"
  },
  "source": "src/index.html",
  "engines": {
    "node": ">= 16",
    "yarn": ">= 3"
  },
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "clean": "rm -rf ./dist && mkdir ./dist",
    "copy-static": "cp -r ./public/. ./dist",
    "start": "yarn parcel",
    "build": "yarn parcel build",
    "test": "yarn jest",
    "test:dev": "yarn test --watch --onlyChanged",
    "lint": "yarn eslint src/ --ext ts,js,tsx,jsx,mjs,cjs --color",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "yarn build-storybook",
    "prepare": "is-ci || husky install",
    "format": "yarn prettier --write '**/**/*.{ts,js,tsx,jsx,cjs,mjs,css,json,md,mdx,html,yaml,yml}'"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@headlessui/react": "1.6.6",
    "@sentry/react": "7.11.1",
    "@sentry/tracing": "7.11.1",
    "clsx": "1.2.1",
    "ethers": "5.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "ress": "5.0.2",
    "wagmi": "0.6.4",
    "web3": "1.7.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@emotion/babel-plugin": "11.10.2",
    "@emotion/eslint-plugin": "11.10.0",
    "@mdx-js/react": "1.6.22",
    "@metamask/jazzicon": "2.0.0",
    "@metamask/onboarding": "1.0.1",
    "@storybook/addon-docs": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/eslint": "8.4.6",
    "@types/events": "3.0.0",
    "@types/jest": "28.1.8",
    "@types/node": "18.11.8",
    "@types/postcss-import": "14.0.0",
    "@types/prettier": "2.7.0",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "babel-loader": "8.2.5",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.4",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "29.0.1",
    "jest-environment-jsdom": "29.0.1",
    "jest-junit": "14.0.1",
    "lint-staged": "13.0.3",
    "parcel": "2.7.0",
    "postcss": "8.4.16",
    "postcss-import": "14.1.0",
